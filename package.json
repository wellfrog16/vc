{
  "name": "@wfrog/vc",
  "version": "0.12.0",
  "author": "wellfrog",
  "license": "ISC",
  "homepage": "https://wellfrog16.gitee.io/vc/",
  "repository": {
    "type": "git",
    "url": "git://github.com/wellfrog16/vc.git",
    "web": "https://github.com/wellfrog16/vc"
  },
  "keywords": [
    "vue",
    "element-plus",
    "element-plus-components",
    "vue3-components"
  ],
  "main": "./es/index.mjs",
  "module": "./es/index.mjs",
  "types": "./es/index.d.ts",
  "files": [
    "dist",
    "es"
  ],
  "engines": {
    "node": "=16.18.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build && npm run build:lib && npm run build:type && npm run provide:css && npm run volar",
    "build:lib": "esno ./build/esbuild.ts",
    "build:type": "tsc --emitDeclarationOnly --declaration --project tsconfig.build.json",
    "provide:css": "esno ./build/libcss",
    "volar": "esno ./build/volar-type",
    "preview": "vite preview",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:deploy-build": "cross-env deploy=gh vitepress build",
    "docs:preview": "vitepress preview",
    "gh-pages": "cross-env deploy=gh npm run docs:build",
    "gh": "node gh-pages",
    "publish2": "npm publish --access public",
    "commit": "git-cz",
    "lint": "commitlint -e",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "dependencies": {
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.25.2",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@element-plus/icons-vue": "^2.3.1",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vueuse/core": "^11.2.0",
    "@wfrog/esbuild-plugin-vue-next": "^0.1.5",
    "@wfrog/stylelint-config-vue-scss": "^1.0.0",
    "@wfrog/utils": "^0.1.0",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cz-git": "^1.3.12",
    "element-plus": "^2.9.11",
    "esbuild": "^0.15.9",
    "eslint": "^8.20.0",
    "esno": "^0.16.3",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "klaw-sync": "^6.0.0",
    "lodash-es": "^4.17.21",
    "sass": "^1.54.0",
    "stylelint": "^14.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-plugin-dts": "^1.4.1",
    "vitepress": "^1.5.0",
    "vue": "^3.2.37",
    "vue-tsc": "^0.38.4"
  }
}
