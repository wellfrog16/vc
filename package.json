{
  "name": "vcbuilder",
  "version": "1.0.0",
  "packageManager": "pnpm@10.20.0",
  "description": "",
  "author": "wellfrog",
  "license": "ISC",
  "keywords": [],
  "main": "index.js",
  "engines": {
    "node": ">=22.17.1"
  },
  "scripts": {
    "dev": "pnpm ui:docs:dev && pnpm playground:dev",
    "ui:build": "pnpm -C packages/ui build",
    "ui:docs:dev": "pnpm -C packages/ui docs:dev",
    "ui:docs:build": "pnpm -C packages/ui docs:build",
    "ui:docs:deploy-build": "pnpm -C packages/ui docs:deploy-build",
    "ui:docs:preview": "pnpm -C packages/ui docs:preview",
    "ui:publish": "pnpm --filter ./packages/ui publish2",
    "utils:build": "pnpm -C packages/utils build",
    "playground:dev": "pnpm -C playground dev",
    "playground:build": "pnpm -C playground build",
    "postinstall": "pnpm utils:build && pnpm ui:build",
    "clean": "pnpm -r run clean && rm -rf node_modules",
    "commit": "git-cz",
    "lint": "commitlint -e",
    "push": "node ./scripts/auto-push.js",
    "gen:com": "esno ./scripts/create-component.ts",
    "gen:fun": "esno ./scripts/create-function.ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "dependencies": {
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@element-plus/icons-vue": "^2.3.2",
    "@types/klaw-sync": "^6.0.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/shared": "^3.5.24",
    "@vueuse/core": "^14.0.0",
    "@wfrog/vc-ui": "workspace:*",
    "@wfrog/vc-utils": "workspace:*",
    "commitizen": "^4.3.1",
    "cross-env": "^10.1.0",
    "cz-git": "^1.12.0",
    "element-plus": "^2.11.7",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.0.2",
    "esno": "^4.8.0",
    "klaw-sync": "^7.0.0",
    "lodash-es": "^4.17.21",
    "rimraf": "^6.1.0",
    "sass-embedded": "^1.93.3",
    "typescript": "~5.9.3",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^3.1.0"
  }
}
