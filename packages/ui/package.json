{
  "name": "@wfrog/vc",
  "version": "1.0.2",
  "packageManager": "pnpm@10.20.0",
  "description": "vue3 组件库 vc",
  "author": "wellfrog",
  "license": "ISC",
  "keywords": ["vue3-components", "vue3-ui", "components", "vue3-component"],
  "exports": {
    ".": {
      "types": "./dist/es/index.d.ts",
      "import": "./dist/es/index.mjs"
    },
    "./index.css": "./dist/index.css",
    "./es/components/*/type": "./dist/es/components/*/*.d.ts",
    "./es/components/*": {
      "types": "./dist/es/components/*/index.d.ts",
      "default": "./dist/es/components/*/index.mjs"
    },
    "./resolver": {
      "types": "./dist/es/utils/resolver.d.ts",
      "default": "./dist/es/utils/resolver.mjs"
    },
    "./*": "./dist/*"
  },
  "main": "dist/es/index.mjs",
  "types": "dist/es/index.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "publishConfig": {
    "directory": "."
  },
  "scripts": {
    "build": "vite build && pnpm volar && pnpm provide:css",
    "provide:css": "esno ./build/libcss",
    "volar": "esno ./build/volar-type",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:deploy-build": "cross-env deploy=gh vitepress build",
    "docs:preview": "vitepress preview",
    "prepublishOnly": "cp src/index.md README.md",
    "publish2": "npm publish --access public",
    "postpublish": "rimraf README.md",
    "clean": "rimraf node_modules dist .vitepress/dist  .vitepress/cache"
  },
  "dependencies": {
    "vuedraggable-es-fix": "^1.0.1"
  },
  "devDependencies": {
    "@types/klaw-sync": "^6.0.5",
    "esbuild": "^0.27.0",
    "esno": "^4.8.0",
    "klaw-sync": "^7.0.0",
    "vitepress": "2.0.0-alpha.15"
  }
}
