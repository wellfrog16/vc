{
  "name": "@wfrog/vc-utils",
  "version": "1.4.0",
  "packageManager": "pnpm@10.20.0",
  "description": "函数工具库 vc-utils",
  "author": "wellfrog",
  "license": "ISC",
  "keywords": [
    "vue3-utils",
    "utils",
    "vue3"
  ],
  "exports": {
    ".": {
      "types": "./dist/es/index.d.ts",
      "import": "./dist/es/index.mjs"
    },
    "./es/functions/*": {
      "types": "./dist/es/functions/*/index.d.ts",
      "default": "./dist/es/functions/*/index.mjs"
    },
    "./*": "./dist/*"
  },
  "main": "dist/es/index.mjs",
  "types": "dist/es/index.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "publishConfig": {
    "directory": "."
  },
  "scripts": {
    "build": "vite build",
    "publish2": "npm publish --access public",
    "clean": "rimraf node_modules dist"
  },
  "dependencies": {
    "@types/crypto-js": "^4.2.2",
    "crypto-js": "^4.2.0"
  }
}
