{
  "name": "@wfrog/vc-utils",
  "version": "1.0.0",
  "packageManager": "pnpm@10.20.0",
  "description": "函数工具库 vc-utils",
  "author": "wellfrog",
  "license": "ISC",
  "keywords": ["vue3-utils", "utils", "vue3"],
  "exports": {
    ".": {
      "types": "./dist/es/index.d.ts",
      "import": "./dist/es/index.mjs"
    },
    "./es/utils/*": {
      "types": "./dist/es/utils/*/index.d.ts",
      "default": "./dist/es/utils/*/index.mjs"
    },
    "./*": "./dist/*"
  },
  "main": "dist/es/index.mjs",
  "types": "dist/es/index.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "publishConfig": {
    "directory": "."
  },
  "scripts": {
    "build": "vite build",
    "prepublishOnly": "cp src/index.md README.md",
    "publish2": "npm publish --access public",
    "postpublish": "rimraf README.md",
    "clean": "rimraf node_modules dist"
  }
}
